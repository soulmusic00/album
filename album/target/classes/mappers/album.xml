<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.album">

	<select id="selectAlbumDetail" parameterType="int" resultType="AlbumDto">
	SElECT
			*
	FROM 
			T_SHOPPING_ALBUM
	WHERE	
			ALBUM_ID = #{albumId}
	</select>
	
</mapper>